{
  "name": "bee-select",
  "version": "2.0.26",
  "description": "select ui component for react",
  "keywords": [
    "react",
    "react-component",
    "react-select",
    "iuap-design",
    "neoui-react",
    "select"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "homepage": "https://github.com/tinper-bee/bee-select",
  "author": "",
  "repository": "http://github.com/tinper-bee/bee-select",
  "bugs": "https://github.com/tinper-bee/select/issues",
  "license": "MIT",
  "main": "./build/index",
  "config": {
    "port": 8000,
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "dev": "bee-tools run start",
    "build": "bee-tools run build",
    "lint": "bee-tools run lint",
    "test": "bee-tools run test",
    "chrome": "bee-tools run chrome",
    "browsers": "bee-tools run browsers",
    "pub": "bee-tools run pub",
    "coveralls": "bee-tools run coverage",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "bee-animate": "^1.0.0",
    "bee-menus": "^2.0.2",
    "bee-overlay": "^1.0.23",
    "classnames": "^2.2.5",
    "component-classes": "1.x",
    "dom-scroll-into-view": "1.x",
    "omit.js": "^1.0.2",
    "rc-menu": "7.4.19",
    "react-lifecycles-compat": "^3.0.4",
    "tinper-bee-core": "latest",
    "warning": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^15.3.0 || ^16.0",
    "react-dom": "^15.3.0 || ^16.0",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "bee-button": "latest",
    "bee-clipboard": "^2.0.0",
    "bee-drawer": "0.0.2",
    "bee-form": "latest",
    "bee-layout": "latest",
    "bee-panel": "latest",
    "chai": "^3.5.0",
    "console-polyfill": "~0.2.1",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^2.4.1",
    "es5-shim": "~4.1.10",
    "react": "~15.3.0",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "~15.3.0",
    "husky": "^4.2.5"
  }
}
