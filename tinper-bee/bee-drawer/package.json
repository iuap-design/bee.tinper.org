{
  "name": "bee-drawer",
  "version": "0.0.7",
  "description": "Drawer ui component for react",
  "keywords": [
    "react",
    "react-component",
    "bee-drawer",
    "iuap-design",
    "tinper-bee",
    "Drawer"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "homepage": "https://github.com/tinper-bee/bee-drawer.git",
  "author": "Yonyou FED",
  "repository": "http://github.com/tinper-bee/bee-drawer",
  "bugs": "https://github.com/tinper-bee/bee-drawer.git/issues",
  "license": "MIT",
  "main": "./build/index.js",
  "config": {
    "port": 3000,
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "dev": "bee-tools run start",
    "build": "bee-tools run build",
    "lint": "bee-tools-test run lint",
    "test": "bee-tools-test run test",
    "chrome": "bee-tools-test run chrome",
    "coveralls": "bee-tools-test run coverage",
    "browsers": "bee-tools-test run browsers",
    "pub": "bee-tools run pub",
    "changelog": "bee-tools run changelog",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "bee-icon": "^1.0.11",
    "classnames": "^2.2.6",
    "react-transition-group": "^2.6.1",
    "tinper-bee-core": "latest"
  },
  "devDependencies": {
    "bee-button": "latest",
    "bee-checkbox": "^2.0.5",
    "bee-clipboard": "^2.0.0",
    "bee-datepicker": "^2.0.28",
    "bee-form": "^2.0.11",
    "bee-form-control": "^2.0.2",
    "bee-label": "^1.0.0",
    "bee-layout": "latest",
    "bee-panel": "latest",
    "bee-radio": "latest",
    "chai": "^3.5.0",
    "console-polyfill": "~0.2.1",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^2.4.1",
    "es5-shim": "~4.1.10",
    "react": "^16.4.2",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "^16.4.2",
    "husky": "^4.2.5"
  },
  "peerDependencies": {
    "react": "^15.3.0 || ^16.0.0",
    "react-dom": "^15.3.0 || ^16.0.0",
    "prop-types": "^15.6.0"
  }
}
